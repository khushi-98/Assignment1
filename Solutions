    for i in range(2, num):
        if num % i == 0:
            break
        else:
            print(num)
            break
#ques 11

import time
start=time.time()
for i in range(3):
    print("Hello")
    
end=time.time()
print("Execution time",end-start)

#ques 12
import time
import random
start=time.time()
l=[]
for i in range(0,5000):
    num= random.randint(10,100)
    l.append(num)
print(l)
l.sort()
print(l)
end=time.time()
print("Execution time",end-start)


